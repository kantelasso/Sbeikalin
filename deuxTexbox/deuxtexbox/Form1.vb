        nonPermis.Show()
    End Function

    Private Sub FullScrToolStripMenuItem_Click_1(sender As Object, e As EventArgs) Handles FullScrToolStripMenuItem.Click
        If FullScrToolStripMenuItem.Text = "ߓߍ߲߬ߒߠߊ" Then

            If pathOuvrir.Visible = False Then
                pleinEcran()
            ElseIf pathOuvrir.Visible = True Then
                defaultEcran()
                pasPermisCarFicherOuvert()
            End If
        ElseIf FullScrToolStripMenuItem.Text = "ߢߊ߫ ߞߘߐ" Then
            If pathOuvrir.Visible = False Then
                defaultEcran()
            End If
        End If
    End Sub
    ' =======================================

    Public Function closeSbeikalin()

        If ecrire.Modified Then
            beforeCloseApp.Show()

            If ouvrir.Modified Then
                beforeCloseApp.Show()
            End If

        ElseIf ouvrir.Modified Then
            beforeCloseApp.Show()
        Else
            Me.Close()
        End If

    End Function

