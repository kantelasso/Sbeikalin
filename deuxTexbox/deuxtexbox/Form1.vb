Public Class Form1
    '=== Création du fichier .txt ===

    Public Function saveTexbox1()

        If SFileDEcrire.ShowDialog = Windows.Forms.DialogResult.OK Then
            Dim SW As New StreamWriter(SFileDEcrire.FileName)
            SW.Write(ecrire.Text)
            SW.Close()
            noSaveEcrire.Text = "ߛߓߍ ߓߘߊ߫ ߟߊߡߙߊ߬"
            noSaveEcrire.ForeColor = Color.DarkGreen
            noSaveEcrire.Visible = True
            Path.GetFileName(SFileDEcrire.FileName)
            pathEcrire.Text = Path.GetFileName(SFileDEcrire.FileName)
            pathEcrire.Visible = True
            ecrireButon.Enabled = False
            saveAgainButon.Visible = True
            ToSaveToolStripMenuItem.Enabled = False
            SaveAgainToolStripMenuItem.Enabled = True

        End If
    End Function

    Private Sub Button1_Click(sender As Object, e As EventArgs) Handles ecrireButon.Click

        saveTexbox1()

    End Sub
