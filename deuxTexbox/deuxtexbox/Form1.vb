    Public Function openTexbox2()
        If OpenFileDialog1.ShowDialog = Windows.Forms.DialogResult.OK Then
            Dim SR As New StreamReader(OpenFileDialog1.FileName)
            ouvrir.Text = SR.ReadToEnd
            Path.GetFileName(OpenFileDialog1.FileName)
            pathOuvrir.Text = Path.GetFileName(OpenFileDialog1.FileName)
            pathOuvrir.Visible = True
            SR.Close()
            OpenOuCréer("ߘߊߦߟߍ߬ߣߍ߲")
            lanceNMO()
        End If
    End Function

    Private Sub Button2_Click(sender As Object, e As EventArgs) Handles ouvrirButon.Click
