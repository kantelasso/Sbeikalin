    'Corriger le texte de la zone Ecrire uniquement
    Public Function corrigeSeulement()
        defaultEcran()
        fautesMinimes()
        noControled.Text = "ߊ߬ ߓߘߊ߫ ߛߊߞߍ߫"
        noControled.ForeColor = Color.DarkGreen
        noControled.Visible = True
    End Function

    '===== Fonction qui corrige les fautes de frappe =====
    Public Function correctionFautes()
        If ouvrir.Visible = False Then
            corrigeSeulement()
            If noSaveEcrire.Visible = True Then

                corrigeSeulement()
            End If

        ElseIf ouvrir.Visible = True Then
            If noSaveOuvrir.Visible = True Then
                If noSaveOuvrir.Text = "ߊ߬ ߓߘߊ߫ ߟߊߡߙߊ߬" Then
                    corrigeSeulement()
                ElseIf noSaveOuvrir.Text = "ߊ߬ ߓߘߊ߫ ߡߊߦߟߍ߬ߡߊ߲߬" Then
                    avantControlBoite.Show()
                ElseIf noSaveEcrire.Visible = True Then
                    corrigeSeulement()
                End If
            ElseIf noSaveOuvrir.Visible = False Then
                corrigeSeulement()
            End If

        End If

    End Function
    ' === Correction à partir du buton control ===
    Private Sub control_Click(sender As Object, e As EventArgs) Handles control.Click
        correctionFautes()
    End Sub

    '=== Correction à partir du menu ===
    Private Sub ControlToolStripMenuItem_Click(sender As Object, e As EventArgs) Handles ControlToolStripMenuItem.Click
