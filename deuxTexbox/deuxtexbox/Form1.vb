    Private Sub Button3_Click(sender As Object, e As EventArgs) Handles saveButon.Click
        If pathOuvrir.Visible = False Then
            CréerdsOuvrir()
        ElseIf pathOuvrir.Visible = True Then
            If OpenOrNew.Text = "ߘߊߦߟߍ߬ߣߍ߲" Then
